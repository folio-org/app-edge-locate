          "PortBindings": {"8080/tcp": [{"HostPort": "%p"}]}
        }},
        "env": [        {
          "name": "JAVA_OPTIONS",
          "value": "-XX:MaxRAMPercentage=66.0"
        }]
      }
    },
        {
      "id": "edge-users-2.1.0",
      "name": "Retrieve and create users information Edge API",
      "provides": [],
      "requires": [      {
        "id": "users",
        "version": "16.0 16.4"
      }],
      "permissionSets": [],
      "launchDescriptor":       {
        "dockerImage": "edge-users-2.1.0",
        "dockerPull": false,
        "dockerArgs": {"HostConfig":         {
          "Memory": 268435456,
          "PortBindings": {"8081/tcp": [{"HostPort": "%p"}]}
        }},
        "env": [        {
          "name": "JAVA_OPTIONS",
          "value": "-XX:MaxRAMPercentage=66.0"
        }]
      }
    }
